    "start": "gulp serve",
    "deploy:stable": "gulp build_lib --mode production --dist_path stable && gulp deploy_lib --dest_branch stable --dist_path stable --git_paths_to_include README.md",
    "deploy:latest": "gulp build_lib --mode production --dist_path latest && gulp deploy_lib --dest_branch latest --dist_path latest --git_paths_to_include README.md",
    "test": "npx jest"
  },
